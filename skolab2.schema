#########################
# Skolab object classes #
#########################

# main skolab server configuration
# storing global values and user specific default values
# like skolabFreeBusyFuture and skolabFreeBusyPast
# FIXME: change OID!
objectclass ( 1.3.6.1.4.1.19414.2.2.1
  NAME 'skolab'
  DESC 'Skolab server configuration'
  SUP top STRUCTURAL
  MUST k
  MAY ( skolabHost $
        postfix-mydomain $
        postfix-relaydomains $
        postfix-mydestination $
        postfix-mynetworks $
        postfix-relayhost $
        postfix-relayport $
        postfix-transport $
        postfix-virtual $
        postfix-enable-virus-scan $
        postfix-allow-unauthenticated $
        postfix-message-size-limit $
        cyrus-quotawarn $
        cyrus-autocreatequota $
        cyrus-admins $
        cyrus-imap $
        cyrus-pop3 $
        cyrus-imaps $
        cyrus-pop3s $
        cyrus-sieve $
        cyrus-smmap $
        cyrus-fulldirhash $
        cyrus-hashimapspool $
        cyrus-squatter $
        apache-http $
        apache-allow-unauthenticated-fb $
        skolabfilter-verify-from-header $
        skolabfilter-allow-sender-header $
        skolabfilter-reject-forged-from-header $
        skolabPolicyDaemon $
        skolabSecureCiphers $
        proftpd-ftp $
        proftpd-defaultquota $
        skolabFreeBusyFuture $
        skolabFreeBusyPast $
        uid $
        userPassword ) )

# public folders are typically visible to everyone subscribed to
# the server without the need for an extra login. Subfolders are
# defined using the hiarchy seperator '/' e.g. "sf/sub1". Please note
# that the term public folder is prefered to shared folder because
# normal user mailboxes can also share folders using acls.
# FIXME: change OID!
objectclass ( 1.3.6.1.4.1.19414.2.2.9
  NAME 'skolabSharedFolder'
  DESC 'Skolab public shared folder'
  SUP top STRUCTURAL
  MUST cn
  MAY ( acl $
        alias $
        cyrus-userquota $
        skolabHomeServer $
        skolabFolderType $
        skolabDeleteflag ) )

# skolabNamedObject is used as a plain node for the LDAP tree.
# In contrast to unix filesystem directories LDAP nodes can
# and often do also have contents/attributes. We use the
# skolabNamedObject in order to put some structure in the
# LDAP directory tree.
# FIXME: change OID!
objectclass ( 1.3.6.1.4.1.5322.13.1.1
  NAME 'skolabNamedObject'
  SUP top STRUCTURAL
  MAY (cn $ ou) )

# skolab account
# we use an auxiliary in order to ease integration
# with existing inetOrgPerson objects
# Please note that userPassword is a may
# attribute in the schema but is mandatory for
# Skolab
# FIXME: change OID!
objectclass ( 1.3.6.1.4.1.19414.3.2.2
  NAME 'skolabInetOrgPerson'
  DESC 'Skolab Internet Organizational Person'
  SUP top AUXILIARY
  MAY ( c $
        alias $
        pseudonym $
        skolabHomeServer $
        skolabHomeServerOnly $
        skolabHomeMTA $
        unrestrictedMailSize $
        skolabDelegate $
        skolabEncryptedPassword $
        cyrus-userquota $
        skolabInvitationPolicy $
        skolabFreeBusyFuture $
        skolabVacationBeginDateTime $
        skolabVacationEndDateTime $
        skolabVacationResendInterval $
        skolabVacationAddress $
        skolabVacationReplyToUCE $
        skolabVacationReactDomain $
        skolabForwardAddress $
        skolabForwardKeepCopy $
        skolabForwardUCE $
        skolabAllowSMTPRecipient $
        skolabAllowSMTPFrom $
        skolabSalutation $
        skolabMaritalStatus $
        dateOfBirth $
        placeOfBirth $
        birthName $
        gender $
        homeFacsimileTelephoneNumber $
        countryOfCitizenship $
        countryOfResidence $
        legalForm $
        tradeRegisterLocation $
        tradeRegisterIdentifier $
        VATNumber $
        germanTaxId $
        skolabDeleteflag $
        skolabComment ) )

# skolab organization with country support
# FIXME: change OID!
objectclass ( 1.3.6.1.4.1.19414.3.2.3
  NAME 'skolabOrganization'
  DESC 'RFC2256: a Skolab organization'
  SUP organization STRUCTURAL
  MAY ( c $
        mail $
        skolabDeleteflag $
        alias ) )

# skolab organizational unit with country support
# FIXME: change OID!
objectclass ( 1.3.6.1.4.1.19414.3.2.4
  NAME 'skolabOrganizationalUnit'
  DESC 'a Skolab organizational unit'
  SUP organizationalUnit STRUCTURAL
  MAY ( c $
        mail $
        skolabDeleteflag $
        alias ) )

# skolab groupOfNames with extra skolabDeleteflag and the required
# attribute mail.
# The mail attribute for skolab objects of the type skolabGroupOfNames
# is not arbitrary but MUST be a single attribute of the form
# of an valid SMTP address with the CN as the local part.
# E.g cn@skolabdomain (e.g. employees@mydomain.com). The
# mail attribute MUST be globally unique.
# FIXME: change OID!
objectclass ( 1.3.6.1.4.1.19414.3.2.5
  NAME 'skolabGroupOfNames'
  DESC 'Skolab group of names (DNs) derived from RFC2256'
  SUP groupOfNames STRUCTURAL
  MAY ( mail $
        skolabDeleteflag ) )

# FIXME: change OID!
objectclass ( 1.3.6.1.4.1.19414.3.2.6
  NAME 'skolabExternalPop3Account'
  DESC 'skolab fetch messages via POP3 from external sources'
  SUP top STRUCTURAL
  MUST ( externalPop3AccountServer $
         externalPop3AccountLoginName $
         externalPop3EncryptedAccountPassword )
  MAY ( externalPop3AccountDescription $
        externalPop3AccountMail $
        externalPop3AccountPort $
        externalPop3AccountUseSSL $
        externalPop3AccountUseTLS $
        externalPop3AccountLoginMethod $
        externalPop3AccountCheckServerCertificate $
        externalPop3AccountKeepMailOnServer ) )

