objectclass ( 1.3.6.1.4.1.19414.3.2.7
  NAME 'skolabGermanBankArrangement'
  DESC 'German bank account information'
  SUP top STRUCTURAL
  MUST ( skolabGermanBankAccountNumber $
         skolabGermanBankCode )
  MAY ( skolabGermanBankAccountHolder $
